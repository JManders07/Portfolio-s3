uter from 'vue-router'\n  Vue.use(VueRouter)\n\n  Vue.use(VueAxios, axios, VueRouter)\n  export default {\n      name: 'CreateView',\n      components: {\n      },\n      data() {\n          return {\n              form: {\n                  name: '',\n                  rating: null\n              }\n          }\n      },\n      methods: {\n          onSubmit(event) {\n              event.preventDefault()\n              console.warn(this.form)\n              axios({\n                  method: 'post',\n                  url: 'https://newsbackend.azurewebsites.net/Review/Rate',\n                  data: {\n                      \"comment\": this.form.comment,\n                      \"title\": this.form.articleId,\n                      \"grade\": this.form.rating\n                  }\n              })\n          }\n      }\n  }\n</script>\n\n<style scoped>\n  .rate{\n      margin-left: 50px;\n      margin-bottom: 25px;\n  }\n\n  .rate-comment{\n      margin-top: 25px;\n      margin-bottom: 25px;\n  }\n\n  .btn{\n      margin-left: 10px;\n  }\n\n  .nav{\n      color: white;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}